{
  "expo": {
    "name": "Fahrgut",
    "slug": "fahrgut",
    "version": "1.0.0",
    "platforms": [
      "web",
      "android"
    ],
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.rezaafg.Fahrgut"
    },
    "web": {
      "manifest": {
        "icons": [
          {
            "src": "./assets/icons/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/icons/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      },
      "favicon": "./assets/icon_appstore.png",
      "name": "Fahrgut",
      "startUrl": "/home",
      "scope": "/",
      "bundler": "metro",
      "build": {
        "output": "web-build"
      },
      "pwa": true
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9bdf502d-6853-490a-b310-872eba01322f"
      }
    },
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "owner": "reza4030"
  }
}
